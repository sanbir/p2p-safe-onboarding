{
  "name": "@p2p-org/safe-onboarding-sdk",
  "version": "1.0.0",
  "description": "P2P.org Safe onboarding SDK for delegating Superform access via Zodiac Roles",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/p2p-org/p2p-safe-onboarding.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "CHANGELOG.md",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup && tsc --project tsconfig.declarations.json",
    "dev": "tsup --watch",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "prepare": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run typecheck && npm run build"
  },
  "keywords": [
    "safe",
    "zodiac",
    "roles",
    "superform",
    "p2p"
  ],
  "dependencies": {
    "@safe-global/safe-deployments": "^1.37.48",
    "dotenv": "^16.4.5",
    "viem": "^2.9.8",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20.16.11",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "tsup": "^8.0.2",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=18"
  }
}
